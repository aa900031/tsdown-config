{
	"name": "@aa900031/tsdown-config",
	"type": "module",
	"version": "0.2.0",
	"packageManager": "pnpm@10.28.1",
	"author": "zhong666 <hi@zhong666.me>",
	"license": "MIT",
	"homepage": "https://github.com/aa900031/tsdown-config#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aa900031/tsdown-config.git",
		"directory": "packages/core"
	},
	"bugs": {
		"url": "https://github.com/aa900031/tsdown-config/issues"
	},
	"sideEffects": false,
	"exports": {
		".": "./src/index.ts",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"import": {
					"types": "./dist/index.d.mts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.cts",
					"default": "./dist/index.cjs"
				}
			},
			"./package.json": "./package.json"
		}
	},
	"scripts": {
		"lint": "eslint . --cache --cache-location ./node_modules/.cache/eslint",
		"release": "release-it",
		"build": "tsdown",
		"test": "vitest run",
		"bench": "vitest bench --run --project benchmark"
	},
	"peerDependencies": {
		"tsdown": "^0.0.0"
	},
	"devDependencies": {
		"@aa900031/eslint-config": "^6.0.0",
		"@aa900031/release-it-config": "^0.1.1",
		"@tsconfig/node22": "^22.0.5",
		"@types/node": "^22.19.7",
		"@vitest/coverage-istanbul": "^3.2.4",
		"eslint": "^9.39.3",
		"release-it": "^19.2.4",
		"tsdown": "^0.20.3",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	}
}
